{
    "name": "svgx",
    "productName": "SVGX",
    "version": "1.1.0",
    "description": "SVG icon manager",
    "main": "src/index.js",
    "scripts": {
        "start": "concurrently \"npm:svelte-dev\" \"electron-forge start\"",
        "start-demo": "export \"DEMO=yes\" && concurrently \"npm:svelte-dev\" \"electron-forge start\"",
        "start-win-demo": "set \"DEMO=yes\" && concurrently \"npm:svelte-dev\" \"electron-forge start\"",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "make:mac": "concurrently \"npm:svelte-build\" \"npx @electron-forge/cli make --platform darwin\"",
        "make:win": "concurrently \"npm:svelte-build\" \"npx @electron-forge/cli make --platform win32\"",
        "make:linux": "concurrently \"npm:svelte-build\" \"npx @electron-forge/cli make --platform linux\"",
        "publish": "electron-forge publish",
        "lint": "echo \"No linting configured\"",
        "svelte-build": "rollup -c",
        "svelte-dev": "rollup -c -w",
        "svelte-start": "sirv public",
        "test": "jest"
    },
    "keywords": [],
    "author": {
        "name": "breadthe",
        "email": "svgx@protonmail.com"
    },
    "license": "MIT",
    "config": {
        "forge": "forge.config.js"
    },
    "dependencies": {
        "electron-reload": "^1.5.0",
        "electron-squirrel-startup": "^1.0.0",
        "focus-visible": "^5.1.0",
        "highlight.js": "^9.18.1",
        "html2canvas": "^1.0.0-rc.7",
        "huebee": "^2.0.0",
        "node-sass": "^5.0.0",
        "readdirp": "^3.4.0",
        "sirv-cli": "^0.4.4",
        "svelte-click-outside": "^1.0.0",
        "svgo": "^1.3.2"
    },
    "devDependencies": {
        "@babel/core": "^7.10.1",
        "@babel/preset-env": "^7.10.1",
        "@electron-forge/cli": "^6.0.0-beta.50",
        "@electron-forge/maker-deb": "^6.0.0-beta.50",
        "@electron-forge/maker-dmg": "^6.0.0-beta.50",
        "@electron-forge/maker-rpm": "^6.0.0-beta.50",
        "@electron-forge/maker-squirrel": "^6.0.0-beta.50",
        "@electron-forge/maker-zip": "^6.0.0-beta.50",
        "@fullhuman/postcss-purgecss": "^2.1.0",
        "@rollup/plugin-commonjs": "^12.0.0",
        "@rollup/plugin-node-resolve": "^8.0.0",
        "autoprefixer": "^9",
        "babel-jest": "^26.0.1",
        "concurrently": "^5.1.0",
        "electron": "^9",
        "jest": "^26.0.1",
        "postcss": "^7",
        "postcss-import": "^12.0.1",
        "rollup": "^1.32.0",
        "rollup-plugin-livereload": "^1.0.0",
        "rollup-plugin-node-builtins": "^2.1.2",
        "rollup-plugin-postcss": "^3.1.1",
        "rollup-plugin-svelte": "^5.0.3",
        "rollup-plugin-terser": "^6.1.0",
        "svelte": "^3.19.1",
        "svelte-jester": "^1.0.6",
        "tailwindcss": "npm:@tailwindcss/postcss7-compat"
    }
}
